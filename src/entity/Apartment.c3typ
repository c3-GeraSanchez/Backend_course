/** 
* Apartment.c3typ
* A apartment in a {@link Building} with many {@link Fixture}s
*/
entity type Apartment mixes FeatureEvaluatable {

    /**
     * The building this apartment is in
     */ 
    building: !Building
    
    /**
     * The fixtures in the apartment.
     */ 
    fixtures: [Fixture](apartment)

    // Return the monthly average temperature of SmartBulbs in an {@link Apartment}
    averageTemperatureMonth: member function(endDate: datetime): double js-server

}