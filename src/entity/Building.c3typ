/** 
* Building.c3typ
* A single {@link Building} containing multiple {@Apartment}s.
*/
entity type Building mixes FeatureEvaluatable {

    /**
     * The city where this building is
     */ 
    city: !City
    
    /**
     * The apartments in the building
     */ 
    apartments: [Apartment](building)
    
    /**
     * Latitude location where the building is 
     */  
    latitude: !double
    
    /**
     * Longitud location where the building is 
     */ 
    longitude: !double

}